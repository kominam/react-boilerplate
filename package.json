{
  "name": "react-boilerplate",
  "description": "Boilerplate for React with Webpack",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "cross-env NODE_ENV=production webpack",
    "lint": "eslint ./src",
    "lint:fix": "yarn lint --fix",
    "pretty": "prettier --loglevel warn --write \"*.{js,json}\" \"{src,config,cypress}/**/*.{js,json}\"",
    "test": "jest --config=config/tests/jest.config.js",
    "test:coverage": "yarn test --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "storybook": "start-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kominam/react-boilerplate.git"
  },
  "keywords": [
    "React",
    "Webpack",
    "Jest",
    "Storybook",
    "Terraform"
  ],
  "author": "kominam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kominam/react-boilerplate/issues"
  },
  "homepage": "https://github.com/kominam/react-boilerplate#readme",
